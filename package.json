{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:beckend": "tsx dev-backend.ts",
    "dev:frontend": "nuxt dev",
    "dev": "concurrently \"npm run dev:beckend\" \"npm run dev:frontend\"",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --ui --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix && prettier './**/*.{html,css,less,scss}' --write",
    "tsc": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "nodemon": "^3.1.4",
    "nuxt": "^3.12.4",
    "openapi-fetch": "^0.10.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.2",
    "@loongwoo/stc": "^1.6.1",
    "@vitest/coverage-istanbul": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue-hook-optimizer": "^0.0.63",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-typegen": "^0.2.4",
    "happy-dom": "^14.12.3",
    "openapi-typescript": "^7.1.0",
    "tsx": "^4.16.2",
    "vitest": "^2.0.4",
    "vue-eslint-parser": "^9.4.3"
  }
}
